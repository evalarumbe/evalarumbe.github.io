{
  "name": "eva-larumbe",
  "version": "2.0.0",
  "description": "Freelance web professional based in Berlin.",
  "main": "index.html",
  "scripts": {
    "dev": "dart-sass --watch ./styles/scss/main.scss ./style.css",
