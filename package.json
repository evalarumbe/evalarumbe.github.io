  "license": "ISC",
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "dart-sass": "^1.25.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0"
  }
}
